{
    "env": {
        "es2021": true,
        "node": true
    },
    "extends": [
        "google",
        "plugin:@typescript-eslint/recommended"
    ],
    "parser": "@typescript-eslint/parser",
    "parserOptions": {
        "ecmaVersion": 12,
        "sourceType": "module"
    },
    "plugins": [
        "@typescript-eslint",
        "eslint-plugin-tsdoc"
    ],
    "rules": {
      "camelcase": "off",
      "max-len": "off",
      "require-jsdoc": "off",
      "valid-jsdoc": "off",
      "no-invalid-this": "off"
    }
}
